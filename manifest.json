{
    "manifest_version": 3,
    "name": "My Chrome Extension",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "content_scripts":[
        {
            "matches":["https://developer.chrome.com/*"],
            "js":["src/content.js"]
        }
    ],
    "action": {
      "default_popup": "src/popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "background": {
      "service_worker": "src/background.js"
    }
  }